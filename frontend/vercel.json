{
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://otvinsight.com/admin/:path*",
      "permanent": false,
      "has": [
        {
          "type": "host",
          "value": "admin.otvinsight.com"
        }
      ]
    }
  ],
  "rewrites": [
    { 
      "source": "/(admin|nominate|profile|vote|signup|login|categories|about|nomination-success|nomination-card/.*|about-us|contact-us|terms|price-detail|refund-policy|privacy-policy)", 
      "destination": "/index.html" 
    }
  ]
}