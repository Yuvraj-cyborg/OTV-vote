{
  "rewrites": [
    {
      "source": "/(nominate|profile|vote|signup|login|categories|about|nomination-success|nomination-card/.*|about-us|contact-us|terms|price-detail|refund-policy)", 
      "destination": "/index.html" 
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "admin.*"
        }
      ],
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}