{
  "rewrites": [
    {
      "source": "/(nominate|profile|vote|signup|login|categories|about|nomination-success|nomination-card/.*|about-us|contact-us|terms|price-detail|refund-policy|admin)", 
      "destination": "/index.html" 
    },
